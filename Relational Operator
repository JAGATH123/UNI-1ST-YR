/* C Relational Operations on integers */

#include <stdio.h>

int main()
{
  int a = 9;
  int b = 4;
  
  printf(" a >  b: %d \n", a > b);
  printf("a >= b: %d \n", a >= b);
  printf("a <= b: %d \n", a <= b);
  printf("a <  b: %d \n", a < b);
  printf("a == b: %d \n", a == b);
  printf("a != b: %d \n", a != b);

}
